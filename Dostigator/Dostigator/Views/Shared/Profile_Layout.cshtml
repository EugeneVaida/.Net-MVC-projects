
@{
    ViewBag.Title = "Profile_Layout";
}
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<nav>
    <div class="nav-wrapper grey darken-3">
        <a href="#!" class="brand-logo">Logo</a>
        <a href="#" data-target="mobile-demo" class="sidenav-trigger"><i class="material-icons">menu</i></a>
        <ul class="right hide-on-med-and-down">
            <li><a href="">Материалы</a></li>
            <li><a href="">Лента</a></li>
            <li><a href="">Группы</a></li>
        </ul>
    </div>
</nav>

<ul class="sidenav" id="mobile-demo">
    <li><a href="">Материалы</a></li>
    <li><a href="">Лента</a></li>
    <li><a href="">Группы</a></li>    
</ul>

<div class="container">
    <section>
        <ul id="slide-out" class="sidenav">
            <li>
                <div class="user-view">
                    <div class="background">
                        <img src="https://drscdn.500px.org/photo/177823087/w%3D440_h%3D440/v2?webp=true&v=7&sig=9f050ea7d86827347c212238c04f24d9ea34d94d335f7f33614c5b0e2a33b3b9">
                    </div>

                    <a href="#user"><img class="circle" src="https://drscdn.500px.org/photo/195143459/h%3D1065/v2?webp=true&sig=c9db37fe0017a0479f01bb2a010c88ef4748bcb3a9ef4c58494c20a3fd945fc6"></a>
                    <a href="#name"><span class="white-text name">@Model.Email</span></a>
                    <a href="#email"><span class="white-text email">@Model.Age</span></a>


                </div>
            </li>
            <li><a href="#!" class="waves-effect">Мои цели</a></li>
            <li><a href="#!" class="waves-effect">Таймлайн</a></li>
            <li><a href="#!" class="waves-effect">Вызовы</a></li>
            <li><div class="divider"></div></li>
            <li><a class="subheader">Subheader</a></li>
            <li><a class="waves-effect" href="#!">Third Link With Waves</a></li>
        </ul>

        <a href="#" data-target="slide-out" class="sidenav-trigger"><i class="medium material-icons">menu</i></a>
    </section>
</div>





@RenderBody()



<footer class="page-footer grey darken-2">
    <div class="footer-copyright">
        <div class="container">
            Made by Eugene Vaida
        </div>
    </div>
</footer>






